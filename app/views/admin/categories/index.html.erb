<section class="admin-products-index">

  <header class="page-header">
    <h1>Admin &raquo; All Categories</h1>
  </header>

  <div class="well">
    <%= link_to '+ New Categorie', [:new, :admin, :category], class: 'btn btn-info' %>
    </div>

  <div class="alert alert-info" role="alert">
    Categories in our store:</p>
  </div>

  <% @categories.each do |category| %>
    <li class="list-group-item list-group-item-info"> <b><i><%= category.name%></b></i> </li>
  <% end %>

</section>

